<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Storage</title>
    @Styles.Render("~/Content/css")
</head>
<body>
    @Html.Action("Index", "Menu", new { categ =  ViewContext.RouteData.Values["categ"]} )
    <div class="container body-content">
        @RenderBody()
        <footer class="mt-4">
            <hr />
            <p>&copy; @DateTime.Now.Year - PASHKARKASTAN Studio</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
</body>
</html>
